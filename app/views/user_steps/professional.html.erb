<div class="panel panel-default">
  <div class="panel-heading">
   <h3 class="panel-title">Pfofessional Information</h3>
  </div>

  <div class="panel-body">
    <%= bootstrap_form_for current_user, url: wizard_path , html: { multipart: true } do |f| %>
      <%= f.text_field :industry %>
      <%= f.text_field :education_details %>
      <%= f.text_field :previous_company_details %>
      <%= f.file_field :avatar %>
      <%= f.submit "Continue", :class => 'btn btn-primary' %>
    <% end %>
  </div>  
</div>
